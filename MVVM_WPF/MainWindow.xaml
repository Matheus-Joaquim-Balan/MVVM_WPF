<Window x:Class="MVVM_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MVVM_WPF"
        xmlns:views="clr-namespace:MVVM_WPF.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" FontSize="14" Width="800">
    <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto">
        <Grid Margin="25">
            <views:YoutubeViewerView DataContext="{Binding YoutubeViewersViewModel}"/>
        </Grid>
    </ScrollViewer>
</Window>
